#depts-timeline
  margin 0 auto 10px auto
  background-color white
  border-radius 10px
  box-shadow 0 0 8px
  padding 5px 0px
  font-size 20px
  width 95%
  .order-in
    width 100%
    text-align start
    padding 3px 10px
    font-size 16px
  .dept-wrap
    padding 5px 15px
    .dept
      display inline-block
      width 26%
      .img-wrap
        display inline-block
        height 40px
        width 40px
        float left
        img
          height 40px
          width 40px
      h1
        display inline-block
        padding 7px 5px
    .data
      display inline-block
      width 74%
      border-top 1px solid tomato
      border-bottom 1px solid tomato
      .desc
        width 80%
        display inline-block
      .price
        color tomato
        width 20%
        display inline-block
