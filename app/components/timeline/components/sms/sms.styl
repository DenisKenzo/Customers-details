#sms-timeline
  margin 0 auto 10px auto
  width 95%
  .order-in
    display inline-block
    float left
    font-size 20px
  .icon-wrap
    display inline-block
    width 30px
    height 20px
    padding 5px
    float left
    img
      width 20px
      height 20px
  .text
    display inline-block
    text-overflow ellipsis
    line-height 25px
    overflow hidden
    margin-top -1px
    width 70%
