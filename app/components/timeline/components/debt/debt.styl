#debts-timeline
  margin 0 auto 10px auto
  border-radius 10px
  box-shadow 0 0 8px
  padding 5px 0px
  font-size 20px
  width 95%
  .order-in
    width 100%
    text-align start
    padding 3px 10px
    font-size 16px
  .debt-wrap
    padding 5px 15px
    .debt
      display inline-block
      width 26%
      .img-wrap
        display inline-block
        height 40px
        width 40px
        float left
        img
          height 40px
          width 40px
      h1
        display inline-block
        padding 7px 5px
    .data
      display inline-block
      width 74%
      .desc
        text-overflow ellipsis
        display inline-block
        line-height 23px
        overflow hidden
        padding 3px
        height 23px
        width 69%
      .price
        display inline-block
        width 31%
