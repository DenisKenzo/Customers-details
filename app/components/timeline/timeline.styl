#timeline
  background-color gainsboro
  overflow hidden
  width 100%
  position relative
  .list
    padding-top 10px
    min-height 736px
    width 100%
    margin-bottom 100px
    .separator-wrap
      padding-bottom 10px
      width 100%
      .separator
        border-top 1px solid lightslategray
        border-bottom 1px solid lightslategray
        width 80%
        margin 0 auto
        text-align center
        color deepskyblue
    .spiner-wrap
      width 100%
      height 50px
      img
        margin 0 auto
        display block
        width 50px
        height 50px
  .buttons-wrap
    background-color gainsboro
    width 100%
    height 70px
    position fixed
    bottom 0
    div
      background-color darkslategray
      display inline-block
      text-align center
      margin 10px 1px
      padding 5px 10px
      border none
      color white
      height 50px
      img
        width 20px
        height 20px
      h1
        font-size 15px
        margin-top -3px
  .nVisible
    visibility hidden
