#timeline
  background-color gainsboro
  overflow hidden
  width 100%
  position relative
  .list
    padding-top 10px
    width 100%
  .test
    height 1000px
  .separator-wrap
    padding-bottom 10px
    width 100%
    .separator
      border-top 1px solid lightslategray
      border-bottom 1px solid lightslategray
      width 80%
      margin 0 auto
      text-align center
      color deepskyblue
  .buttons-wrap
    background-color gainsboro
    width 100%
    height 80px
    position fixed
    bottom 0
    div
      background-color darkslategray
      display inline-block
      text-align center
      margin 10px 1px
      padding 5px 10px
      font-size 20px
      border none
      color white
      height 60px
      img
        width 20px
        height 20px
      h1
        margin-top -3px
  .nVisible
    visibility hidden
