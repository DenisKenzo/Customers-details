#gallery
  border-bottom 1px solid grey
  height 900px
  width 100%
  .add-button
    float right
    width 80px
    margin 20px 40px 0 20px
  .add-label
    float right
    margin 40px 20px 0 20px
    color #6b6b6b
  .label-wrap
    width 100%
    height 100px
    h1
      float left
      margin 40px 0 0 30px
      font-weight 500
      font-size 32px
      color #6b6b6b
    .gallery-label
      width 130px
      height 36px
      border-radius 18px
      background-color #e8ffff
      margin 40px 30px 0 0
      text-align center
      font-size 20px
      padding-top 3px
      float right
      color #6b6b6b
  #swiper-wrap-gallery
    .swiper-container
      position static
      img
        padding 30px
        width 100%
        height 100%
      .swiper-pagination
        transform translateY(-700px)
        bottom auto
        .swiper-pagination-bullet
          width 20px
          height 20px
        .swiper-pagination-bullet-active
          background #fc351f
