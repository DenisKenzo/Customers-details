#adress
  border-bottom 1px solid grey
  height 230px
  .google-map
    height 200px
    width 200px
    margin-top 20px
    margin-left 20px
    float left
    img
      width 100%
      height 100%
  .adress-label
    padding 5px 20px 5px 20px
    border-radius 20px
    background-color #e8ffff
    margin-top 20px
    margin-right 30px
    text-align center
    font-size 20px
    float right
    color #6b6b6b
  .address
    float right
    margin-top 70px
    transform translateX(70px)
    font-weight 500
    text-align right
    color #304f63
    font-size 36px
    text-decoration none
