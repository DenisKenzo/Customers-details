#adress
  border-bottom 1px solid grey
  height 230px
  .google-map
    height 200px
    width 200px
    margin-top 20px
    margin-left 20px
    float left
    img
      width 100%
      height 100%
  .adress-label
    padding 5px 20px 5px 20px
    border-radius 20px
    background-color #e8ffff
    margin-top 20px
    margin-right 30px
    text-align center
    font-size 20px
    float right
    color #6b6b6b
  .address
    width 25%
    float right
    img
      width 80px
      height 75px
      transform translate3D(-10px,85px,0px)
    h1
      float right
      margin-top 10px
      transform translateX(70px)
      font-weight 500
      text-align right
      color #304f63
      font-size 36px
      text-decoration none
