#comment
  border-bottom 1px solid silver
  height 90px
  .label-wrap
    width 100%
    height 30px
    .add-label
      padding 0px 20px
      height 20px
      border-radius 10px
      background-color #e8ffff
      margin 5px 10px 0 10px
      text-align center
      font-size 14px
      color #6b6b6b
  .add-wrap
    height 60px
    cursor pointer
    img
      width 40px
      margin 10px 30px 0 30px
      display block
    h1
      margin 18px -10px
      color #6b6b6b
      display block
