      float right
      margin 40px 40px 0 20px
  .bar
    border-bottom 1px solid grey
    background-color #fffbdf
    height 380px
    h1
      float right
      margin 30px 30px 0 0
  .block1
    border-bottom 1px solid grey
    height 200px
    .block1-label
      width 480px
      height 36px
      border-radius 18px
      background-color #e8ffff
      margin-top 20px
      margin-right 30px
      text-align right
      font-size 20px
      padding-top 3px
      float right
    button
      float right
      margin 90px -480px 0 0
      height 70px
      width 160px
      border-radius 35px
      background-color white
      border 1px solid gray
      box-shadow 0px 3px 8px gray
      font-size 42px
    h1
      float right
      margin 50px 80px 0 20px
  .block2
    border-bottom 1px solid grey
    height 340px
    input
      float right
