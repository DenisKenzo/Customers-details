#home
  .header
    background-color #ffcd00
    height 100px
    padding-top 20px
    position relative
    text-align center
    .arrow-back
      color white
      cursor pointer
      width 30px
    .icon-online
      background #09ce17
      border 2px solid white
      border-radius 50%
      display inline-block
      transform translateY(2px)
      height 30px
      width 30px
    h1
      color white
      display inline-block
      font-size 42px
      margin 0 auto
      padding-left 5px
      padding-top 10px
    .edit
      background-color #ffcd00
      border none
      color white
      font-size 42px
      &:focus
        outline none
  .buttons
    text-align center
    div
      padding-left 0
      padding-right 0
    button
      background-color #b8954c
      border none
      height 100px
      width 100%
      color #dbc89d
      font-size 42px
      margin 0 auto
      &:focus
        outline none
    .activeCustomers
      background-color #ffb300
      color white
  .hero
    margin-top 80px
    position relative
    .client-img
      pointer-events none
      user-select none
      width 100%
      //to-do object-fit
      z-index 1
    .star
      width 70px
      color #6d6d6d
      position absolute
      top 40px
      left 20px
    .input-group
      bottom 0
      height 80px
      position absolute
      background: rgba(0,0,0,0.3)
      width 100%
      .input-group-addon
        background: rgba(0,0,0,0.0)
        border none
        cursor pointer
        visibility visible
        img
          width 50px
      .input-group-addon-hidden
        visibility hidden
      .input-wrap
        padding 15px
        .form-control
          background  white
          color #00b6fb
          font-size 36px
          font-weight 600
          height 50px
          text-align center
          &:focus
            background: rgba(0,0,0,0.0)
            text-align right
            border none
            box-shadow none
            outline none
        .form-control-disabled
          pointer-events none
  .events
    background-color #e8ffff
    padding-bottom 10px
    height 270px
    border-bottom 1px solid grey
    .clock
      width 80px
      margin 10px 0 0 20px
    .babel
      float right
      padding-right 10px
    #swiper-wrap-notes
      .swiper-container
        position static
        margin-top 10px
        .swiper-wrapper
          padding-left 30px
          .swiper-slide
            width 300px!important
            margin-right 0px!important
        .swiper-pagination
          transform translateY(-210px)
          bottom auto
          .swiper-pagination-bullet
            width 20px
            height 20px
          .swiper-pagination-bullet-active
            background #fc351f
  .call
    border-bottom 1px solid grey
    height 140px
    img
      width 80px
      margin 20px 0 20px 20px
    .call-label
      width 90px
      height 32px
      border-radius 16px
      background-color #e8ffff
      margin-top 20px
      margin-right 10px
      text-align center
      font-size 20px
      padding-top 1px
      float right
    h1
      float right
      margin-top 65px
      transform translateX(50px)
      color #00B6FB
      font-weight 500
  .mail
    border-bottom 1px solid grey
    height 120px
    img
      width 80px
      margin 20px 0 20px 20px
    .mail-label
      width 260px
      height 36px
      border-radius 18px
      background-color #e8ffff
      margin-top 20px
      margin-right 10px
      text-align center
      font-size 20px
      padding-top 3px
      float right
    h1
      float right
      margin-top 65px
      transform translateX(220px)
      color #00B6FB
      font-weight 500
  .map
    border-bottom 1px solid grey
    height 230px
    .map-label
      width 120px
      height 36px
      border-radius 18px
      background-color #e8ffff
      margin-top 20px
      margin-right 10px
      text-align center
      font-size 20px
      padding-top 3px
      float right
    h1
      float right
      margin-top 70px
      transform translateX(110px)
      font-weight 500
      text-align right
  .add
    border-bottom 1px solid grey
    height 120px
    img
      float right
      width 80px
      margin 20px 40px 0 20px
    h1
      float right
      margin 40px 40px 0 20px
  .add1
    border-bottom 1px solid grey
    height 180px
    .add1-label
      width 130px
      height 36px
      border-radius 18px
      background-color #e8ffff
      margin-top 20px
      margin-right 10px
      text-align center
      font-size 20px
      padding-top 3px
      float right
    img
      float right
