      margin 80px 40px 0 20px
      transform translateX(140px)
    h1
      float right
      margin 100px 40px 0 20px
      transform translateX(140px)
  .gallery
    border-bottom 1px solid grey
    height 660px
    width 100%
    .label-wrap
      width 100%
      height 100px
      h1
        float left
        padding-left 30px
        margin-top 30px
        font-weight 500
        font-size 32px
      .gallery-label
        width 130px
        height 36px
        border-radius 18px
        background-color #e8ffff
        margin-top 30px
        margin-right 20px
        text-align center
        font-size 20px
        padding-top 3px
        float right
    #swiper-wrap-gallery
      margin-top 30px
      .swiper-container
        position static
        .swiper-wrapper
          padding-left 30px
          .swiper-slide
            width 300px!important
            margin-right 0px!important
        .swiper-pagination
          transform translateY(-450px)
          bottom auto
          .swiper-pagination-bullet
            width 20px
            height 20px
          .swiper-pagination-bullet-active
            background #fc351f
    img
      float right
      width 80px
      margin 40px 40px 0 20px
    h1
      float right
      margin 60px 40px 0 20px
  .add2
    border-bottom 1px solid grey
    height 120px
    img
      float right
