#details
  border-bottom 1px solid grey
  height 200px
  .details-label
    padding 5px 20px 5px 20px
    border-radius 20px
    background-color #e8ffff
    margin-top 20px
    margin-right 30px
    text-align right
    font-size 20px
    float right
    color #6b6b6b
  .details-button
    float right
    margin 90px -408px 0 0
    height 70px
    width 160px
    border-radius 35px
    background-color white
    border 1px solid gray
    box-shadow 0px 4px 8px gray
    font-size 42px
    &:focus
      outline none
  h1
    float right
    margin 53px 80px 0 20px
    color #6b6b6b
