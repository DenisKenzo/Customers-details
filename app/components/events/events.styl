#events
  background-color #e8ffff
  height 170px
  border-bottom 1px solid grey
  .clock
    width 40px
    margin 5px 0 0 10px
  .label
    float right
    margin-right 10px
    margin-top 10px
    color #6b6b6b
    background-color #e8ffff
    border none
    &:focus
      outline none
  #swiper-wrap-notes
    .swiper-container
      position static
      width 100%
      .swiper-wrapper
        .swiper-slide:nth-of-type(1)
          width 22%!important
        .swiper-slide
          padding 0px 7px 0 7px
          width 39%!important
          .start-note
            background-color lightskyblue!important
            padding-top 10px!important
            font-size 22px
            text-align center
            color darkslategray
          .note
            background-color white
            border-radius 10px
            width 100%
            height 110px
            border 1px solid #bcbcbc
            box-shadow 1px 3px 6px #bcbcbc
            display inline-block
            padding 5px
            h1
              margin 0
            .date
              color dimgray
              line-height 20px
              height 40px
              width 100%
            .procedure
              line-height 20px
              height 40px
              width 100%
              color darkslategray
              overflow hidden
              text-overflow ellipsis
            .price
              color deepskyblue
              height 20px
              width 100%
          .end-note
            background-color gainsboro!important
            font-size 32px
            color darkslategray
            .orange
              color orangered
      .swiper-pagination
        transform translateY(-147px)
        bottom auto
        .swiper-pagination-bullet
          width 11px
          height 11px
          margin 0 2px
        .swiper-pagination-bullet-active
          background #fc351f
